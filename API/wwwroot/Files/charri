    * scroll chaining in parent element. overscroll-behavior parameter doesn't work
    * https://jira.corp.adobe.com/browse/XD-132461
    */
    transition: opacity 150ms ease-out;
}
.ReactModal__Overlay--after-open {
    opacity: 1;
    transition: opacity 150ms ease-out;
}

.ReactModal__Content {
    -webkit-transform: scale(0.5) rotateX(-30deg);
            transform: scale(0.5) rotateX(-30deg);
}

