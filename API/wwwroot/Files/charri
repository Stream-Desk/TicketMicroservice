        opacity: 0;
        visibility: visible;
    }

    15% {
        opacity: 1;
        visibility: visible;
    }

    50% {
        opacity: 1;
        visibility: visible;
    }

    80% {
        opacity: 1;
        visibility: visible;
    }

    99.9% {
        opacity: 0;
        visibility: visible;
    }

    100% {
        visibility: hidden;
        opacity: 1;
    }
}

